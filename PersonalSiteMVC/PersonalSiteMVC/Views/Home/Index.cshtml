<!-- ======= Contact Section ======= -->
<section id="contact" class="contact">
    <div class="container">
        @*!STEP 10 - Display the success message, if one exists*@
        @if (!string.IsNullOrEmpty(ViewBag.Success))
        {
            <div class="alert alert-success alert-dismissible" role="alert">
                @Html.Raw(ViewBag.Success)
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>
        }
        @*!STEP 11 - Display the error message, if one exists*@
        @if (!string.IsNullOrEmpty(ViewBag.ErrorMessage))
        {
            <div class="alert alert-danger alert-dismissible" role="alert">
                @Html.Raw(ViewBag.ErrorMessage)
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>
        }
        <div class="section-title">
            <h2>Contact</h2>
            <p>Contact Me</p>
        </div>

        <div class="row mt-2">

            <div class="col-md-6 mt-4 d-flex align-items-stretch">
                <div class="info-box">
                    <i class="bx bx-envelope"></i>
                    <h3>Email Me</h3>
                    <p>jason.pinkston.it@outlook.com</p>
                </div>
            </div>

            <div class="col-md-6 mt-4  d-flex align-items-stretch">
                <div class="info-box">
                    <i class="bx bx-share-alt"></i>
                    <h3>Social Profiles</h3>
                    <div class="social-links">
                        <a href="https://github.com/jpinky82" target="_blank" class="github"><i class="bi bi-github"></i></a>
                        <a href="https://www.linkedin.com/in/jason-pinkston-48224243/" target="_blank" class="linkedin"><i class="bi bi-linkedin"></i></a>
                    </div>
                </div>
            </div>

            <form asp-action="Contact" method="post" role="form" class="php-email-form mt-4">
                <div class="row">
                    <div class="col-md-6 form-group">
                        <input asp-for="Name" class="form-control" id="name" placeholder="Your Name" required>
                        <span asp-validation-for="Name" class="text-danger"></span>
                    </div>
                    <div class="col-md-6 form-group mt-3 mt-md-0">
                        <input asp-for="Email" class="form-control" id="email" placeholder="Your Email" required>
                        <span asp-validation-for="Email" class="text-danger"></span>
                    </div>
                </div>
                <div class="form-group mt-3">
                    <input asp-for="Subject" class="form-control" id="subject" placeholder="Subject" required>
                    <span asp-validation-for="Subject" class="text-danger"></span>
                </div>
                <div class="form-group mt-3">
                    <textarea asp-for="Message" class="form-control mb-1" rows="5" placeholder="Message" required></textarea>
                    <span asp-validation-for="Message" class="text-danger"></span>
                </div>

                <div class="form-group text-center"><button type="submit">Send Message</button></div>
            </form>

        </div>
</section><!-- End Contact Section -->
